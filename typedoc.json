{
  "entryPoints": [
    "app/*",
    "app/**/*"
  ],
  "out": "docs",
  "exclude": [
    "**/node_modules/**",
    "**/*.test.ts"
  ],
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "hideGenerator": true,
  "plugin": [
    "typedoc-plugin-markdown"
  ],
  "theme": "default",
  "readme": "README.md",
  "categorizeByGroup": true,
  "disableSources": false,
  "excludeExternals": true
}